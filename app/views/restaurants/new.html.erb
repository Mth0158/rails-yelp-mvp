<h1>Add a new restaurant</h1>
<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :category %>
  <%= f.select :category,  %w[chinese italian japanese french belgian] %>
  <%= f.submit %>
<% end %>
